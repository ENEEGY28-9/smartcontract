{
  "dashboard": {
    "title": "Game Metrics Dashboard",
    "tags": ["game", "metrics", "gameplay"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "schemaVersion": 16,
    "version": 1,
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Active Players",
        "type": "stat",
        "targets": [
          {
            "expr": "gateway_active_game_sessions",
            "legendFormat": "Players Online"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 100 },
                { "color": "red", "value": 500 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Active Rooms",
        "type": "stat",
        "targets": [
          {
            "expr": "room_manager_active_rooms",
            "legendFormat": "Active Rooms"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 10 },
                { "color": "red", "value": 50 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 6, "y": 0 }
      },
      {
        "id": 3,
        "title": "Matchmaking Queue",
        "type": "stat",
        "targets": [
          {
            "expr": "room_manager_matchmaking_queue_depth",
            "legendFormat": "Queue Depth"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 20 },
                { "color": "red", "value": 100 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 }
      },
      {
        "id": 4,
        "title": "Game Latency",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(gateway_game_latency_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 0.05 },
                { "color": "red", "value": 0.1 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 18, "y": 0 }
      },
      {
        "id": 5,
        "title": "Worker Frame Time",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(worker_frame_time_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 0.02 },
                { "color": "red", "value": 0.05 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 8 }
      },
      {
        "id": 6,
        "title": "Game Events/sec",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(worker_gameplay_events_total[5m])",
            "legendFormat": "Events/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 1000 },
                { "color": "red", "value": 5000 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 6, "y": 8 }
      },
      {
        "id": 7,
        "title": "Prediction Accuracy",
        "type": "stat",
        "targets": [
          {
            "expr": "gateway_game_prediction_accuracy * 100",
            "legendFormat": "Accuracy %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                { "color": "red", "value": null },
                { "color": "yellow", "value": 80 },
                { "color": "green", "value": 90 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 12, "y": 8 }
      },
      {
        "id": 8,
        "title": "Database Connection Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "gateway_database_connections{status=\"active\"} / gateway_database_connections{status=\"total\"} * 100",
            "legendFormat": "Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 70 },
                { "color": "red", "value": 90 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 18, "y": 8 }
      },
      {
        "id": 9,
        "title": "Players Online Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "gateway_active_game_sessions",
            "legendFormat": "Active Players"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 }
      },
      {
        "id": 10,
        "title": "Room Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "room_manager_active_rooms",
            "legendFormat": "Active Rooms"
          },
          {
            "expr": "room_manager_matchmaking_queue_depth",
            "legendFormat": "Queue Depth"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 }
      },
      {
        "id": 11,
        "title": "Game Latency Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(gateway_game_latency_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(gateway_game_latency_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(gateway_game_latency_seconds_bucket[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 }
      },
      {
        "id": 12,
        "title": "Worker Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(worker_frame_time_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(worker_frame_time_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 }
      },
      {
        "id": 13,
        "title": "Game Events by Type",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(worker_gameplay_events_total{event_type=\"player_input\"}[5m])",
            "legendFormat": "Player Input"
          },
          {
            "expr": "rate(worker_gameplay_events_total{event_type=\"game_state\"}[5m])",
            "legendFormat": "Game State"
          },
          {
            "expr": "rate(worker_gameplay_events_total{event_type=\"collision\"}[5m])",
            "legendFormat": "Collisions"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 32 }
      },
      {
        "id": 14,
        "title": "RPC Call Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(worker_rpc_call_duration_seconds_bucket[5m]))",
            "legendFormat": "RPC 95th percentile"
          },
          {
            "expr": "rate(worker_rpc_calls_total[5m])",
            "legendFormat": "RPC Calls/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 32 }
      },
      {
        "id": 15,
        "title": "Database Query Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(gateway_database_query_duration_seconds_bucket[5m]))",
            "legendFormat": "Query 95th percentile"
          },
          {
            "expr": "rate(gateway_database_queries_total[5m])",
            "legendFormat": "Queries/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 40 }
      },
      {
        "id": 16,
        "title": "Cache Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(redis_cache_hits_total[5m]) / (rate(redis_cache_hits_total[5m]) + rate(redis_cache_misses_total[5m])) * 100",
            "legendFormat": "Hit Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 40 }
      }
    ],
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "templating": {
      "list": [
        {
          "name": "room_type",
          "type": "query",
          "query": "label_values(room_manager_active_rooms, room_type)",
          "refresh": 1,
          "multi": true,
          "includeAll": true
        },
        {
          "name": "event_type",
          "type": "query",
          "query": "label_values(worker_gameplay_events_total, event_type)",
          "refresh": 1,
          "multi": true,
          "includeAll": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Game Events",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(worker_gameplay_events_total[1h]) > 100",
          "iconColor": "orange",
          "textFormat": "High game activity detected"
        }
      ]
    },
    "description": "Game Metrics Dashboard - Real-time gameplay and performance monitoring"
  }
}
