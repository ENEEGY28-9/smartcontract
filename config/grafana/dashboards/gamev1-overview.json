{
  "dashboard": {
    "title": "GameV1 Overview Dashboard",
    "tags": ["gamev1", "overview"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "15s",
    "schemaVersion": 16,
    "version": 2,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Gateway Response Time",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(gateway_response_time_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 0.1 },
                { "color": "red", "value": 0.5 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Active Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "gateway_active_connections",
            "legendFormat": "Active"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 100 },
                { "color": "red", "value": 500 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 6, "y": 0 }
      },
      {
        "id": 3,
        "title": "Worker Frame Time",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(worker_frame_time_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 0.02 },
                { "color": "red", "value": 0.05 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 }
      },
      {
        "id": 4,
        "title": "RPC Call Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(worker_rpc_calls_total[5m])",
            "legendFormat": "Calls/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 100 },
                { "color": "red", "value": 1000 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 18, "y": 0 }
      },
      {
        "id": 5,
        "title": "Active Rooms",
        "type": "stat",
        "targets": [
          {
            "expr": "room_manager_active_rooms",
            "legendFormat": "Rooms"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 5 },
                { "color": "red", "value": 20 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 8 }
      },
      {
        "id": 6,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(gateway_http_requests_failed_total[5m]) / rate(gateway_http_requests_total[5m]) * 100",
            "legendFormat": "Error %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 1 },
                { "color": "red", "value": 5 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 6, "y": 8 }
      },
      {
        "id": 7,
        "title": "Gateway Response Time Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(gateway_response_time_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(gateway_response_time_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(gateway_response_time_seconds_bucket[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 }
      },
      {
        "id": 8,
        "title": "Worker Frame Time Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(worker_frame_time_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(worker_frame_time_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 }
      },
      {
        "id": 9,
        "title": "Connection Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "gateway_active_connections{connection_type=\"websocket\"}",
            "legendFormat": "WebSocket"
          },
          {
            "expr": "gateway_active_connections{connection_type=\"websocket_game\"}",
            "legendFormat": "Game WebSocket"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 }
      },
      {
        "id": 10,
        "title": "Game Events",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(worker_gameplay_events_total[5m])",
            "legendFormat": "Events/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 }
      },
      {
        "id": 11,
        "title": "Rate Limit Hits",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(gateway_rate_limited_requests_total[5m])",
            "legendFormat": "Hits/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 5 },
                { "color": "red", "value": 20 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 32 }
      },
      {
        "id": 12,
        "title": "Memory Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "gateway_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "MB"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 256000000 },
                { "color": "red", "value": 512000000 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 6, "y": 32 }
      },
      {
        "id": 13,
        "title": "Database Connection Pool",
        "type": "stat",
        "targets": [
          {
            "expr": "gateway_database_connections{status=\"active\"}",
            "legendFormat": "Active"
          },
          {
            "expr": "gateway_database_connections{status=\"total\"}",
            "legendFormat": "Total"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 12, "y": 32 }
      },
      {
        "id": 14,
        "title": "Authentication Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(gateway_auth_success_total[5m]) / (rate(gateway_auth_success_total[5m]) + rate(gateway_auth_failure_total[5m])) * 100",
            "legendFormat": "Success %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                { "color": "red", "value": null },
                { "color": "yellow", "value": 90 },
                { "color": "green", "value": 95 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 18, "y": 32 }
      },
      {
        "id": 15,
        "title": "Request Rate by Endpoint",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(gateway_http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 40 }
      },
      {
        "id": 16,
        "title": "Error Rate by Service",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(gateway_http_requests_failed_total[5m])",
            "legendFormat": "Gateway Errors"
          },
          {
            "expr": "rate(worker_rpc_calls_total{status=\"error\"}[5m])",
            "legendFormat": "Worker Errors"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "errors/s"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 40 }
      },
      {
        "id": 17,
        "title": "System Resources",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[5m]) * 100",
            "legendFormat": "CPU %"
          },
          {
            "expr": "process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "Memory MB"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 48 }
      },
      {
        "id": 18,
        "title": "Active Game Sessions",
        "type": "graph",
        "targets": [
          {
            "expr": "gateway_active_game_sessions",
            "legendFormat": "Active Sessions"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 48 }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "templating": {
      "list": [
        {
          "name": "service",
          "type": "query",
          "query": "label_values(up, job)",
          "refresh": 1,
          "multi": true,
          "includeAll": true
        },
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up, instance)",
          "refresh": 1,
          "multi": true,
          "includeAll": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(deployment_info[1h]) > 0",
          "iconColor": "blue",
          "textFormat": "Deployment: {{ deployment_name }}"
        }
      ]
    },
    "description": "GameV1 Overview Dashboard - Real-time monitoring of all services"
  }
}
