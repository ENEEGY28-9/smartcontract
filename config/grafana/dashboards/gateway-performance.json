{
  "dashboard": {
    "title": "Gateway Performance Dashboard",
    "tags": ["gateway", "performance", "api"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "10s",
    "schemaVersion": 16,
    "version": 1,
    "time": {
      "from": "now-30m",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Gateway Response Time",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(gateway_response_time_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 0.1 },
                { "color": "red", "value": 0.5 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Requests per Second",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(gateway_http_requests_total[5m])",
            "legendFormat": "Req/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 1000 },
                { "color": "red", "value": 5000 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 6, "y": 0 }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(gateway_http_requests_failed_total[5m]) / rate(gateway_http_requests_total[5m]) * 100",
            "legendFormat": "Error %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 1 },
                { "color": "red", "value": 5 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 }
      },
      {
        "id": 4,
        "title": "Active WebSocket Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "gateway_active_connections{connection_type=\"websocket\"}",
            "legendFormat": "WS Connections"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 500 },
                { "color": "red", "value": 1000 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 18, "y": 0 }
      },
      {
        "id": 5,
        "title": "Memory Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "gateway_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "MB"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 256000000 },
                { "color": "red", "value": 512000000 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 8 }
      },
      {
        "id": 6,
        "title": "Rate Limit Hits",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(gateway_rate_limited_requests_total[5m])",
            "legendFormat": "Hits/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 10 },
                { "color": "red", "value": 50 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 6, "y": 8 }
      },
      {
        "id": 7,
        "title": "Authentication Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(gateway_auth_success_total[5m]) / (rate(gateway_auth_success_total[5m]) + rate(gateway_auth_failure_total[5m])) * 100",
            "legendFormat": "Success %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                { "color": "red", "value": null },
                { "color": "yellow", "value": 90 },
                { "color": "green", "value": 95 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 12, "y": 8 }
      },
      {
        "id": 8,
        "title": "Database Connection Pool",
        "type": "stat",
        "targets": [
          {
            "expr": "gateway_database_connections{status=\"active\"}",
            "legendFormat": "Active"
          },
          {
            "expr": "gateway_database_connections{status=\"total\"}",
            "legendFormat": "Total"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 18, "y": 8 }
      },
      {
        "id": 9,
        "title": "Response Time Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(gateway_response_time_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(gateway_response_time_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(gateway_response_time_seconds_bucket[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 }
      },
      {
        "id": 10,
        "title": "Request Rate by Endpoint",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(gateway_http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 }
      },
      {
        "id": 11,
        "title": "Connection Types",
        "type": "graph",
        "targets": [
          {
            "expr": "gateway_active_connections{connection_type=\"websocket\"}",
            "legendFormat": "WebSocket"
          },
          {
            "expr": "gateway_active_connections{connection_type=\"websocket_game\"}",
            "legendFormat": "Game WebSocket"
          },
          {
            "expr": "gateway_active_connections{connection_type=\"http\"}",
            "legendFormat": "HTTP"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 }
      },
      {
        "id": 12,
        "title": "Error Rate by Endpoint",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(gateway_http_requests_failed_total[5m]) / rate(gateway_http_requests_total[5m]) * 100",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 }
      },
      {
        "id": 13,
        "title": "Rate Limiting Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(gateway_rate_limited_requests_total[5m])",
            "legendFormat": "Rate Limited"
          },
          {
            "expr": "gateway_rate_limit_remaining{type=\"ip\"}",
            "legendFormat": "IP Limit Remaining"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 32 }
      },
      {
        "id": 14,
        "title": "Authentication Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(gateway_auth_success_total[5m])",
            "legendFormat": "Auth Success"
          },
          {
            "expr": "rate(gateway_auth_failure_total[5m])",
            "legendFormat": "Auth Failure"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 32 }
      },
      {
        "id": 15,
        "title": "System Resources",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[5m]) * 100",
            "legendFormat": "CPU %"
          },
          {
            "expr": "process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "Memory MB"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 40 }
      },
      {
        "id": 16,
        "title": "Database Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(gateway_database_query_duration_seconds_bucket[5m]))",
            "legendFormat": "Query 95th percentile"
          },
          {
            "expr": "rate(gateway_database_connections_created_total[5m])",
            "legendFormat": "New Connections/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 40 }
      }
    ],
    "time": {
      "from": "now-30m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "templating": {
      "list": [
        {
          "name": "endpoint",
          "type": "query",
          "query": "label_values(gateway_http_requests_total, endpoint)",
          "refresh": 1,
          "multi": true,
          "includeAll": true
        },
        {
          "name": "method",
          "type": "query",
          "query": "label_values(gateway_http_requests_total, method)",
          "refresh": 1,
          "multi": true,
          "includeAll": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(deployment_info{job=\"gamev1-gateway\"}[1h]) > 0",
          "iconColor": "blue",
          "textFormat": "Gateway Deployment: {{ deployment_name }}"
        }
      ]
    },
    "description": "Gateway Performance Dashboard - Detailed API and connection monitoring"
  }
}
